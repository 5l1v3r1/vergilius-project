extends /partials/page

block title
    title Vergilius Project
    -var selected = 'Kernels';
block content
    .container
        .cmd
            ul.list-inline.breadcrumb(type='none')
                li.list-inline-item
                    | >
                    a(href="/" tabindex="-1")  Home
                li.list-inline-item
                    a(href="/kernels" tabindex="-1") Kernels
                li.list-inline-item #{osname}
    .container.indent
        .mainblock
            p(style='font-size:28px') Structures:
            div
                ul(type='none')
                    for i in structs
                        li: p
                            a(class='listos' href="/os/#{osname}/type/#{i.name}" tabindex="-1") #{i.name}
            p(style='font-size:28px') Enums:
            div
                ul(type='none')
                    for i in enums
                        li: p
                            a(class='listos' href="/os/#{osname}/type/#{i.name}" tabindex="-1") #{i.name}
            p(style='font-size:28px') Unions:
            div
                ul(type='none')
                    for i in unions
                        li: p
                            a(class='listos' href="/os/#{osname}/type/#{i.name}" tabindex="-1") #{i.name}

extends /partials/page

block title
    title Vergilius Project

block content
    .container
        h5 Structures:
        div
            ul
                for i in structs
                    li: p
                        a(href="/os/#{osname}/type/#{i.name}") #{i.name}
        h5 Enums:
        div
            ul
                for i in enums
                    li: p
                        a(href="/os/#{osname}/type/#{i.name}") #{i.name}
        h5 Unions:
        div
            ul
                for i in unions
                    li: p
                        a(href="/os/#{osname}/type/#{i.name}") #{i.name}

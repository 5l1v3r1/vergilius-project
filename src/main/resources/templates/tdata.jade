extends /partials/page

block title
    title Vergilius Project
block content
    nav.site-header
        .site-header__container.header-sticky
            a.header-logo__image(href='/')
            ul.site-header__links
                li
                    a.header-link.active(href='/') Home
                li
                    a.header-link(href='/about') About
                li
                    a.header-link(href='/admin') Admin
    .container
        .cmd
            ul.list-inline.breadcrumb(type='none')
                li.list-inline-item
                    | >
                    a(href="/")  Home
                li.list-inline-item
                    a(href="/os/#{osname}") #{osname}
                li.list-inline-item.active #{i.name}
    .container
        .mainblock
            p(style="font-size:28px; font-weight:700")
                |#{i.name}
                button#copy-button.button(data-clipboard-target='#copyblock').btn-info copy
            script(type='text/javascript').
                (function () {
                    new Clipboard('#copy-button');
                })();
            #copyblock
                ul
                    for i in res1
                        //li(type="none"): p(style='white-space:pre;') #{i}
                        pre
                            code(class="c") !{i}
                            //escaping, changed from "#"
                ul
                    for j in res2
                        //li(type="none"): p(style='white-space:pre;') #{j}
                        pre
                            code(class="c") !{j}
                ul
                    for k in res3
                        //li(type="none"): p(style='white-space:pre;') #{k}
                        pre
                            code(class="c") !{k}